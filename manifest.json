{
  "manifest_version": 2,

  "name": "Linkedin Helper",
  "description": "Foo",
  "version": "0.1",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
   "background": {
     "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ]
}
